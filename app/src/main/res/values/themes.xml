<resources xmlns:tools="http://schemas.android.com/tools">
	<!-- Base application theme. -->
	<style name="Theme.Base" parent="Theme.MaterialComponents.Light.NoActionBar">
		<!-- Primary brand color. -->
		<item name="colorPrimary">@color/yellow_29</item>
		<item name="colorPrimaryVariant">@color/yellow_29</item>
		<item name="colorOnPrimary">@color/white</item>
		<!-- Secondary brand color. -->
		<item name="colorSecondary">@color/black_1C</item>
		<item name="colorSecondaryVariant">@color/black</item>
		<item name="colorOnSecondary">@color/black_transparent_10</item>
		<!-- Status bar. -->
		<item name="android:windowTranslucentStatus">true</item>
		<item name="android:windowLightStatusBar" tools:targetApi="m">true</item>
		<!-- Navigation bar. -->
		<item name="android:windowTranslucentNavigation">true</item>
		<item name="android:windowLightNavigationBar" tools:targetApi="o_mr1">true</item>
		<!-- Customize your theme here. -->
		<item name="colorTransparent">@color/transparent</item>
	</style>
</resources>